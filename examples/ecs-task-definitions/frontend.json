{
  "family": "cat-frontend",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::277679348386:role/cat-cluster-task-execution-role",
  "taskRoleArn": "arn:aws:iam::277679348386:role/cat-cluster-task-role",
  "containerDefinitions": [
    {
      "name": "frontend",
      "image": "277679348386.dkr.ecr.ap-northeast-2.amazonaws.com/cat-frontend:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 3000,
          "protocol": "tcp"
        }
      ],
      "environment": [
        {
          "name": "ENV",
          "value": "dev"
        },
        {
          "name": "PORT",
          "value": "3000"
        },
        {
          "name": "REACT_APP_API_URL",
          "value": "http://cat-alb-xxx.ap-northeast-2.elb.amazonaws.com"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/cat-frontend",
          "awslogs-region": "ap-northeast-2",
          "awslogs-stream-prefix": "ecs",
          "awslogs-create-group": "true"
        }
      },
      "healthCheck": {
        "command": ["CMD-SHELL", "curl -f http://localhost:3000 || exit 1"],
        "interval": 30,
        "timeout": 5,
        "retries": 3,
        "startPeriod": 60
      }
    }
  ]
}
